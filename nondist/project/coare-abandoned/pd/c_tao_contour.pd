!
! Fancy generic plot description intended to work with filled or line 
! contours.  Uses line contours by default.  Platform and field need to be
! filled in.
!
global
	pd-name:		c_tao_contour
c_tao_contour
	platform:		tao
	representation:		contour
	field:			tdry
	icon-left-menu:		standard-cap-left-menu
	icon-middle-menu:	FieldMenu
!	field-select-command:	ContourFieldSelect
!	field-menu-extras:	mono|Monochrome contours
	icon-right-menu:	contour-right
	require:		contour, simple-color
	scale-mode:		auto
	color-table:		Contour
	color:			yellow
	icon:			tao
	do-labels:		true
	label-blanking:		false
	limit-proc:		contour-putup-adj
	max-fill:		5
	radius:			6
	grid-method:		barnes
