MFVERSION="$Id: Imakefile,v 1.1 1992-08-20 13:29:03 granger Exp $"

LIBS = $(LIBZEB) $(LIBRDSS) $(LIBNETCDF)

SRCS = p3_ingest.c
OBJS = p3_ingest.o

DISTFILES = Imakefile $(SRCS) 

NormalProgramTargetNoInstall(p3_ingest,$(OBJS),$(DEPLIBS),$(LIBS),$(SYSLIBS))

InstallProgramWithFlags(p3_ingest,$(BINDIR),-s)

DependTarget()

LintTarget()

DistfilesTarget($(DISTFILES))

