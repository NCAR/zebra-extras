!
! All the definitions required to make the iconbars work.
!
set ui$bitmap_directory concat(getenv("ZEB_TOPDIR"), "/lib/icons")
!
! This is the facilitymenu itself.....
!
define widget facilitymenu menubar " "
	!
	! All this stuff tells the system to put it, unadorned by headers and
	! title bars, where we want it.  DM does "override" by itself now,
	! since this facilitymenu appears in the display config, so we don't
	! bother with it here.
	!
	noheader
	size 338 65
	location 15 15
	! vertical
	override
	!
	! Now we can define the menus themselves.
	!
	menu 'global' bitmap global
		title 'Zeb Control Options'
		line
		entry 'Facility Menus' 'popdown instrumentmenu; popup facilitymenu'
		entry 'Instrument Menus' 'popdown facilitymenu; popup instrumentmenu'
		entry 'Clear Zeb Display' 'display empty'
		entry 'Shut down Display system' 'shutdown'
	endmenu
	menu 'utilities' bitmap utilities
		title 'Utilities'
		line
		entry 'DataStore Widget'\
			'shell "$BINHOME/dsdwidget&"'
		entry 'EventLogger Widget'\
			'shell "$BINHOME/EventLogger&"'
		entry 'Time Control Window'\
			'popup time'
	endmenu
	menu 'central' bitmap central
		title 'Central Facility Instruments'
		line
		submenu 'TWR21X' twr21x
		submenu 'BSRN1' bsrn1
		submenu 'MWR010' mwr10
		submenu 'SMOS8' smos8
		submenu 'SONDE1' sonde1
		submenu 'MFRSR1' mfrsr1
		submenu '915RASS1' 915rass1
		submenu 'EBBR1' ebbr1
	endmenu
	menu 'extended' bitmap extended
		title 'Extended Facility List'
		line
		submenu 'ASHTON' ash
		submenu 'CEMENT' cement
		submenu 'COLDWATER' cold
		submenu 'CORDELL' cord
		submenu 'MEEKER' meek
		submenu 'PLEVNA' plev
		submenu 'RINGWOOD' ring
		submenu 'ELK FALLS' elk
		submenu 'PAWHUSKA' paw
	endmenu
	menu 'boundary' bitmap boundary
		title 'Boundary Facility List'
		line
		submenu 'HILLSBORO' hills
		submenu 'MORRIS' morris
		submenu 'VICI' vici
	endmenu
	menu 'auxiliary' bitmap auxiliary
		title 'Auxiliary Facility List'
		line
	endmenu
enddef

!
! This is the instrumentmenu itself.....
!
define widget instrumentmenu menubar " "
	!
	! All this stuff tells the system to put it, unadorned by headers and
	! title bars, where we want it.  DM does "override" by itself now,
	! since this instrumentmenu appears in the display config, so we don't
	! bother with it here.
	!
	noheader
	size 560 65
	location 15 15
	! vertical
	override
	!
	! Now we can define the menus themselves.
	!
	menu 'global' bitmap global
		title 'Zeb Control Options'
		line
		entry 'Facility Menus' 'popdown instrumentmenu; popup facilitymenu'
		entry 'Instrument Menus' 'popdown facilitymenu; popup instrumentmenu'
		entry 'Clear Zeb Display' 'display empty'
		entry 'Shut down Display system' 'shutdown'
	endmenu
	menu 'utilities' bitmap utilities
		title 'Utilities'
		line
		entry 'DataStore Widget'\
			'shell "$BINHOME/dsdwidget&"'
		entry 'EventLogger Widget'\
			'shell "$BINHOME/EventLogger&"'
		entry 'Time Control Window'\
			'popup time'
	endmenu
	menu 'EBBR' bitmap ebbr
		title 'EBBR Instruments'
		line
		submenu "EBBR1 Central" instebbr1
		submenu "EBBR2 Cement" instebbr2
		submenu "EBBR3 Ringwood" instebbr3
		submenu "EBBR4 Ashton" instebbr4
		submenu "EBBR5 Coldwater" instebbr5
		submenu "EBBR6 Meeker" instebbr6
		submenu "EBBR7 Cordell" instebbr7
		submenu "EBBR8 Plevna" instebbr8
		submenu "EBBR9 Elk Falls" instebbr9
		submenu "EBBR10 Pawhuska" instebbr10
	endmenu
	menu 'SMOS' bitmap smos
		title 'SMOS Instruments'
		line
		submenu "SMOS1 Ashton" instsmos1
		submenu "SMOS2 Coldwater" instsmos2
		line
		submenu "SMOS4 Meeker" instsmos4
		submenu "SMOS5 Ringwood" instsmos5
		line
		submenu "SMOS8 Central" instsmos8
	endmenu
	menu 'BSRN' bitmap bsrn
		title 'BSRN Instruments'
		line
		submenu "BSRN1 Central" instbsrn1
	endmenu
	menu 'MWR' bitmap mwr
		title 'MWR Instruments'
		line
		submenu "MWR004 Vici" instmwr4
		submenu "MWR010 Central" instmwr10
		submenu "MWR011 Hillsboro" instmwr11
		submenu "MWR012 Morris" instmwr12
	endmenu
	menu 'SONDE' bitmap sonde
		title 'SONDE Instruments'
		line
		submenu "SONDE Central" instsonde1
		submenu "SONDE Hillsboro" instsonde81
		submenu "SONDE Vici" instsonde84
		submenu "SONDE Morris" instsonde85
	endmenu
	menu 'TWR21X' bitmap twr21x
		title 'TWR21X Instruments'
		line
		submenu "TWR21X1 Central" insttwr21x
	endmenu
	menu 'MFRSR' bitmap mfrsr
		title 'MFRSR Instruments'
		line
		submenu "MFRSR1 Central" instmfrsr1
	endmenu
	menu '915RASS' bitmap 915rass
		title '915RASS Instruments'
		line
		submenu "915RASS1 Central" inst915rass1
	endmenu
enddef

!
! Facility Menus
!
define widget ash intmenu 'ASHTON'
	title 'Ashton Facility Instruments'
	line
	submenu "EBBR4" ebbr4
	submenu "SMOS1" smos1
endmenu

define widget cement intmenu 'CEMENT'
	title 'Cement Facility Instruments'
	line
	submenu "EBBR2" ebbr2
endmenu

