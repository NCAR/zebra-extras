	submenu 'SONDE B4' sonde84
endmenu

!
! Platforms Menus
!
define widget twr21x intmenu 'TWR21X PLATFORM'
	title 'TWR21X1 PLATFORM'
	line
	entry '1 Minute A0: temp, rh, vap_pres, aspirator'\
		'put_4parameters "Dsgp1twr21xC1.a0" "temp" "rh" "vap_pres" "aspirator"'
	entry '1 Minute A0: vbat'\
		'put_1parameters "Dsgp1twr21xC1.a0" "vbat"'
	entry '30 Minute A1: temp, rh, vap_pres, aspirator'\
		'put_4parameters "Dsgp30twr21xC1.a1" "temp" "rh" "vap_pres" "aspirator"'
	entry '30 Minute A1: sd_temp, sd_rh, sd_vap_pres'\
		'put_3parameters "Dsgp30twr21xC1.a1" "sd_temp" "sd_rh" "sd_vap_pres"'
endmenu

define widget bsrn1 intmenu 'BSRN1'
	title 'Baseline Surface Radiation Network Data'
	line
	entry 'BSRN CF A0: vbat, vref, svref'\
		'put_3parameters "DsgpbsrnC1.a0" "vbat" "vref" "svref"'
	entry 'BSRN CF A0: stcase, stdome, ptcase, ptdome'\
		'put_4parameters "DsgpbsrnC1.a0" "stcase" "stdome" "ptcase" "ptdome"'
	entry 'BSRN CF A0: psp1, psp2, psig, nip'\
		'put_4parameters "DsgpbsrnC1.a0" "psp1" "psp2" "psig" "nip"'
	entry 'BSRN CF A0: spsp1, spsp2, ssig, snip'\
		'put_4parameters "DsgpbsrnC1.a0" "spsp1" "spsp2" "ssig" "snip"'
