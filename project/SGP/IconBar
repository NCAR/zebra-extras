define widget ebbr1 intmenu 'EBBR1'
	title 'Energy Balance Bowen Ratio Data'
	line
	entry '30 Minute A1: sm1-5, ts1-5, shf1-5, c_shf1-5'\
		'put_45parameters "Dsgp30ebbrE13.a1,Dsgp30ebbrE13.a1,Dsgp30ebbrE13.a1,Dsgp30ebbrE13.a1,Dsgp30ebbrE13.a1" "sm1,sm2,sm3,sm4,sm5" "ts1,ts2,ts3,ts4,ts5" "shf1,shf2,shf3,shf4,shf5" "c_shf1,c_shf2,c_shf3,c_shf4,c_shf5"'
	entry '30 Minute A1: g1-5,cs1-5,ces1-5,ave_shf,bowen,e,h,q'\
		'put_45parameters "Dsgp30ebbrE13.a1,Dsgp30ebbrE13.a1,Dsgp30ebbrE13.a1,Dsgp30ebbrE13.a1,Dsgp30ebbrE13.a1" "g1,g2,g3,g4,g5" "cs1,cs2,cs3,cs4,cs5" "ces1,ces2,ces3,ces4,ces5" "ave_shf,bowen,e,h,q"'
	entry '30 Minute A1: home,tair,thum,hum'\
		'put_42parameters "Dsgp30ebbrE13.a1,Dsgp30ebbrE13.a1" "home_15,home_30" "tair_top,tair_bot" "thum_top,thum_bot" "hum_top,hum_bot"'
	entry '30 Minute A1: pres,wind'\
		'put_23parameters "Dsgp30ebbrE13.a1,Dsgp30ebbrE13.a1,Dsgp30ebbrE13.a1" "vp_top,vp_bot,pres" "res_ws,wind_d,sigma_wd"'
	entry '15 Minute A0: mv_hft1-5,rr_sm1-5,rr_ts1-5'\
		'put_35parameters "Dsgp15ebbrE13.a1,Dsgp15ebbrE13.a1,Dsgp15ebbrE13.a1,Dsgp15ebbrE13.a1,Dsgp15ebbrE13.a1" "mv_hft1,mv_hft2,mv_hft3,mv_hft4,mv_hft5" "rr_sm1,rr_sm2,rr_sm3,rr_sm4,rr_sm5" "rr_ts1,rr_ts2,rr_ts3,rr_ts4,rr_ts5"'
	entry '15 Minute A0: rr_thum,mv_hum,tair,bat,signature'\
		'put_42parameters "Dsgp15ebbrE13.a1,Dsgp15ebbrE13.a1" "rr_thum_r,rr_thum_l" "mv_hum_r,mv_hum_l" "tair_r,tair_l" "bat,signature"'
	entry '15 Minute A0: mv_q,mv_pres,mv_wind_d,mv_home'\
		'put_4parameters "Dsgp15ebbrE13.a1" "mv_q" "mv_pres" "mv_wind_d" "mv_home"'
	entry '5 Minute A0: tair,thum,hum,vp'\
		'put_42parameters "Dsgp5ebbrE13.a0,Dsgp5ebbrE13.a0" "tair_top,tair_bot" "thum_top,thum_bot" "hum_top,hum_bot" "vp_top,vp_bot"'
	entry '5 Minute A0: q,pres,home'\
		'put_3parameters "Dsgp5ebbrE13.a0" "q" "pres" "home"'
	entry '5 Minute A0: wind_s,res_ws,wind_d,sigma_wd'\
		'put_4parameters "Dsgp5ebbrE13.a0" "wind_s" "res_ws" "wind_d" "sigma_wd"'
endmenu

