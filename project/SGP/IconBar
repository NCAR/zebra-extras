!
! Procedures for plotting
!
procedure put_4parameters plat1 string field1 string field2 string field3 string field4 string
	display all4
	parameter win1 instrument platform #plat1
	parameter win1 instrument y-field #field1
	parameter win1 instrument axis-l-label #field1
	parameter win1 instrument axis-b-label #plat1
	parameter win2 instrument platform #plat1
	parameter win2 instrument y-field #field2
	parameter win2 instrument axis-l-label #field2
	parameter win2 instrument axis-b-label #plat1
	parameter win3 instrument platform #plat1
	parameter win3 instrument y-field #field3
	parameter win3 instrument axis-l-label #field3
	parameter win3 instrument axis-b-label #plat1
	parameter win4 instrument platform #plat1
	parameter win4 instrument y-field #field4
	parameter win4 instrument axis-l-label #field4
	parameter win4 instrument axis-b-label #plat1
endprocedure

procedure put_3parameters plat1 string field1 string field2 string field3 string
	display all3
	parameter win1 instrument platform #plat1
	parameter win1 instrument y-field #field1
	parameter win1 instrument axis-l-label #field1
	parameter win1 instrument axis-b-label #plat1
	parameter win2 instrument platform #plat1
	parameter win2 instrument y-field #field2
	parameter win2 instrument axis-l-label #field2
	parameter win2 instrument axis-b-label #plat1
	parameter win3 instrument platform #plat1
	parameter win3 instrument y-field #field3
	parameter win3 instrument axis-l-label #field3
	parameter win3 instrument axis-b-label #plat1
endprocedure

