! The following values need to be skipped on various platforms:
!
!	NWS surface hourlies:	-9999
!	Sondes:			-9999
!	NGM:			-99999
!	WPDN:			-999, -99999
!	MWR-LOS:		-9999
!	BLC:			-999, 499376.7
!	SIROS:			-9999
!	MFR:			-1111, -2222, -3333, -4444
!	Profilers:		-9999, 9999

	sgpnwssurfX1.a1-skip-data-points: -9999
