		description 'ddop'
		buttonmap 'big'
		forcepd
	endwindow
	window 'Clock' 5 410 250 30 #clock_exec
		description 'whiteclock'
	endwindow
	widget 'iconbar' 5 445 445 57
	window 'Event logger' 5 772 700 125
		nongraphic
	endwindow
endconfig


