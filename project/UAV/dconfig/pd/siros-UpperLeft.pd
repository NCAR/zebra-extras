!
! This plot description is used for the regular time series fields of the
! SIROS platform.  The platform must be set (depending upon facitlity)
! before this plot description can be used
!
global
	axis-bottom-space:	30
	axis-left-space:	100
	axis-right-space:	100
	axis-top-space:		30
	pd-name:	siros-UpperLeft
	platform:	SOMETHING
	plot-mode:	history
	plot-time:	6-Feb-94,1:18:00
	plot-type:	xygraph
	ta-scale:	.022
	trigger:	platform
	icon-middle-menu:	siros-platforms
	xy-axis-b-font-scale:	10
	xy-axis-l-font-scale:	10
	xy-axis-r-font-scale:	10
	xy-axis-t-font-scale:	10
	xy-data-mode:	series
	xy-series-span:	720m
	require:	siros
xy-grid
	axis-b-label:	Time
	axis-b-tic-interval:	7200s
	axis-bottom:	true
	axis-left:	false
	axis-right:	false
	axis-t-tic-interval:	7200s
	axis-top:	false
	do-side-annotation:	true
	field-color:	red
	icon:	none
	platform:	SOMETHING
	representation:	simple
	representation-style:	line
	x-field:	time
	y-field:	nothing
l_xytrace
	annot-left-menu:	xy-an-left
	annot-middle-menu:	sirosfields
	annot-right-menu:	simple-color
	axis-b-tic-interval:	7200s
	axis-bottom:	false
	axis-l-color:	rgb:ffff/00/00
	axis-l-grid-intensity:	0.7
	axis-left:	true
	axis-right:	false
	axis-t-tic-interval:	7200s
	axis-top:	false
	color-tweaker:	xytrace-all-color
	disable:	false
	do-side-annotation:	true
	field-color:	rgb:ffff/00/00
	icon:	tseries
	icon-color:	rgb:ffff/00/00
	icon-middle-menu:	sirosfields
	icon-right-menu:	xytrace-right
	limit-proc:	xytrace-adjust
!	platform:	DsgpsirosE13.a1
	platform:	SOMETHING
	representation:	simple
	representation-style:	line
	require:	xytrace, simple-color, siros, xyplots
	x-field:	time
	y-field:	up_short_hemisp
r_xytrace
	annot-left-menu:	xy-an-left
	annot-middle-menu:	sirosfields
	annot-right-menu:	simple-color
	axis-b-tic-interval:	7200s
	axis-bottom:	false
	axis-left:	false
	axis-r-color:	rgb:ffff/a5a5/00
	axis-r-grid-intensity:	0.0
	axis-right:	true
	axis-t-tic-interval:	7200s
	axis-top:	false
	color-tweaker:	xytrace-all-color
	disable:	false
	do-side-annotation:	true
	field-color:	rgb:ffff/a5a5/00
	icon:	tseries
	icon-color:	rgb:ffff/a5a5/00
	icon-middle-menu:	sirosfields
	icon-right-menu:	xytrace-right
	limit-proc:	xytrace-adjust
!	platform:	DsgpsirosE13.a1
	platform:	SOMETHING
	representation:	simple
	representation-style:	line
	require:	xytrace, simple-color, siros, xyplots
	x-field:	time
	y-field:	down_short_hemisp
