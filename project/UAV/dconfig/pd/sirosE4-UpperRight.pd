!
! This plot description plots the optical depths SIROS field as a time
! series for each channel, and specifies the menu for changing between
! representations.  The 0 channel gets its own left axis since its values
! are so out of sync with the other channels.
!
global
	axis-bottom-space:	30
	axis-left-space:	100
	axis-right-space:	100
	axis-top-space:		30
	pd-name:	sirosE4-UpperRight
	platform:	DsgpsirosE4.a1
	plot-mode:	history
	plot-time:	now
	plot-type:	xygraph
	ta-scale:	.022
	trigger:	platform
	icon-middle-menu:		DataAvailable
!	icon-middle-menu:	siros-platforms
	xy-axis-b-font-scale:	10
	xy-axis-l-font-scale:	10
	xy-axis-r-font-scale:	10
	xy-axis-t-font-scale:	10
	xy-data-mode:	series
	xy-series-span:	720m
	require:	siros
xy-grid
	axis-b-label:	Time
	axis-b-tic-interval:	7200s
	axis-bottom:	true
	axis-left:	false
	axis-right:	false
	axis-t-tic-interval:	7200s
	axis-top:	false
	do-side-annotation:	true
	field-color:	red
	icon:	none
	platform:	DsgpsirosE4.a1
	representation:	simple
	representation-style:	line
	x-field:	time
	y-field:	nothing
c_channel0
	annot-left-menu:	xy-an-left
	annot-right-menu:	simple-color
	axis-b-tic-interval:	7200s
	axis-bottom:	false
	axis-l-color:	white
	axis-l-grid-intensity:	0.7
	axis-l-label:	Channel 0
	axis-left:	true
	axis-right:	false
	axis-t-tic-interval:	7200s
	axis-top:	false
	color-tweaker:	xytrace-color
	disable:	false
	do-side-annotation:	true
	field-color:	red
	icon:	tseries
	icon-color:	red
!	icon-middle-menu:	siros-optical-plots
	icon-right-menu:	xytrace-right
	limit-proc:	xytrace-adjust
	platform:	DsgpsirosE4.a1
	representation:	simple
	representation-style:	line
	require:	xytrace, simple-color, siros, xyplots
	x-field:	time
	y-field:	hemisp_narrowband
	dimensions:	channel=0
