! Aliases exist to make the name of one field the equivalent to another.
!
alias	altitude	alt
alias	reflectivity	dbz
alias	u_wind	hu
alias 	v_wind	hv
alias	w_wind	hw
alias	u_wind	uwind
alias 	v_wind	vwind
alias	w_wind	wwind
alias	rh	rhum
alias	pt	theta
alias	ept	thetae
alias	ept	theta_e
alias	vt	tvir
alias	tdry	temp
alias	tdry	temperature
alias	wspd	ws
alias	wdir	wd
alias	u_wind	u
alias	v_wind	v
alias	u_wind	ui
alias	v_wind	vi
alias	u_wind	us
alias	v_wind	vs
alias	tdry	ta
alias	dp	td
alias	vt	v_temp
alias	sst	tsst
alias	tdry	atmp
alias	u_wind	wnde
alias	v_wind	wndn
alias	reflectivity	maxdz	! kingsmill
alias	u_wind	u_ctrl
alias	v_wind	v_ctrl
alias	w_wind	w_ctrl
alias	u_wind	udiv
alias	v_wind	vdiv
