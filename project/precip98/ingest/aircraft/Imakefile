LIBS = $(DFALIBS) $(ZLIBS)
SYSLIBS = $(LIBREGEXP)

OBJS = citation.o
SRCS = citation.cc

DISTFILES = Imakefile $(SRCS) citation.cc

CccProgramTarget(citation,$(OBJS),$(DEPLIBS),$(LIBS) $(XLIBS),$(SYSLIBS))

DependTarget()
LintTarget()

DistfilesTarget($(DISTFILES))

