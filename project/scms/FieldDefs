!
! UI-readable file containing definitions of the fields we usually deal with.
!
field	altitude	"altitude"			m
field	cpres0		"altitude-corrected pressure"	mb
field	dp		"dewpoint temperature"		"C"
field	ept		"equiv. pot. temperature"	"K"
field	fstrength	"electric field strength"	v/m
field	mr		"mixing ratio"			g/kg
field	pres		"pressure"			mb
field	pt		"potential temperature"		"K"
field	raina15		"15 minute rain accum."		mm
field	raina16		"16 minute rain accum."		mm
field	reflectivity	"S-band reflectivity"		"dBZ"
field	rh		"relative humidity"		"%"
field	tdry		"ambient temperature"		"C"
field	twet		"wet bulb temperature"		"C"
field	u_wind		"U wind component"		m/s
field	v_wind		"V wind component"		m/s
field	velocity	"doppler velocity"		m/s
field	vt		"virtual temperature"		"K"
field	w_wind		"vertical wind component"	m/s
field	wmax		"max wind"			m/s
field	left		"left azimuth"			"deg"		
field	right		"right azimuth"			"deg"	
field	height		"height"			m
field	wdir		"wind direction"		deg
field	wspd		"wind speed"			m/sec
field	trans		"transponder code"		none
field	pres_w		"pressure vertical velocity"	Pa/s
field	depth		"depth in km"			"km"
field	zdr		"S-band differential reflectivity" "dBZ"
field	xrefl		"X-band reflectivity"		   "dBZ"
field	s-x		"S - X reflectivity difference" "dBZ"
!
! Aliases exist to make the name of one field the equivalent to another.
!
alias	altitude	alt
alias	reflectivity	dbz
alias	reflectivity	srefl
alias	u_wind	hu
alias 	v_wind	hv
alias	w_wind	hw
alias	u_wind	uwind
alias 	v_wind	vwind
alias	w_wind	wwind
alias	rh	rhum
alias	pt	theta
alias	ept	thetae
alias	vt	tvir
alias	tdry	temp
alias	tdry	temperature
alias	wspd	ws
alias	wdir	wd
alias	u_wind	u
alias	v_wind	v
alias	u_wind	ui
alias	v_wind	vi
alias	u_wind	us
alias	v_wind	vs
alias	tdry	ta
alias	dp	td
alias	vt	v_temp
alias	sst	tsst
alias	tdry	atmp
alias	u_wind	wnde
alias	v_wind	wndn
alias	reflectivity	maxdz	! kingsmill
alias	u_wind	u_ctrl
alias	v_wind	v_ctrl
alias	w_wind	w_ctrl
alias	u_wind	udiv
alias	v_wind	vdiv
