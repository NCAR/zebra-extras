!
! Generic plot description for model data winds.  Needs to have the platform
! and icon filled in.
!
global
	pd-name:	p_model_winds
p_model_winds
	platform:	SOMETHING
	icon:		SOMETHING
	altitude-control:	true
	arrow-scale:	0.002
	barb-scale:	0.06
	degrade:	2
	color:		white
	color-mono:	true
	color-table:	gcook16
	grid-method:	bilinear
	icon-left-menu:		standard-cap-left-menu
	icon-middle-menu:	simple-color
	icon-right-menu:	winds-model-right
	limit-proc:	adj_arrow_scale
	line-width:	2
	representation:	vector
	hgrid:		21
	vgrid:		19
	u-field:	u_wind
	v-field:	v_wind
	require:	simple-color, winds
