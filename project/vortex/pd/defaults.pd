	n43rf_lf-icon:		n43rf_lf
	n308d-icon:		n308d
!
! origin at 35N 100W (a convenient point near Norman)
!
	origin-lat:			35
	origin-lon:			-100
	origin-alt:			0.0
!
! The default, big picture zoom locations.
!
	x-min:				-1000
	x-max:				1000
	y-min:				-1000
	y-max:				1000
!
! Default aircraft track colors.
!
	n308d-color:			orange
	n43rf-color:			cyan
!
! Origins for the get position widget.
!
	pos-origin:	norman
!
! Locations.
!
	norman-location:	35.0 -100.0 0.0
!
! Where to put the frame cache files.
!
	file-path:	/tmp
!
! The maximum number of frames to be stored in the file.
!
	max-frames:	30
!
! Commands to be executed out of the DataAvailable menu.
!
	data-available-command:		DefaultDataAvail
